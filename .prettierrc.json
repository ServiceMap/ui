{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 80,
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-css-order", "prettier-plugin-tailwindcss"],
  "tailwindStylesheet": "./src/shared/ui/theme-provider/styles/themes.css",
  "tailwindFunctions": ["clsx", "cva", "cn", "tw"],
  "cssDeclarationSorterOrder": "alphabetical"
}
